---
import AngularLogo from "~icons/logos/angular-icon";
import AstroLogo from "~icons/logos/astro-icon";
import BashLogo from "~icons/logos/bash-icon";
import Css3Logo from "~icons/logos/css-3";
import GitLogo from "~icons/logos/git-icon";
import GithubLogo from "~icons/logos/github-icon";
import GoLogo from "~icons/logos/go";
import Html5Logo from "~icons/logos/html-5";
import JavaScriptLogo from "~icons/logos/javascript";
import NextJsLogo from "~icons/logos/nextjs-icon";
import NodeJsLogo from "~icons/logos/nodejs-icon";
import RustLogo from "~icons/logos/rust";
import SvelteLogo from "~icons/logos/svelte-icon";
import TailwindLogo from "~icons/logos/tailwindcss-icon";
import TypeScriptLogo from "~icons/logos/typescript-icon";
---

<div class="grid grid-cols-3 gap-14 lg:grid-cols-5 [&>svg]:size-full">
  <TypeScriptLogo />
  <AngularLogo />
  <JavaScriptLogo />
  <Html5Logo class="fill-foreground" />
  <Css3Logo class="fill-foreground" />
  <TailwindLogo />
  <NextJsLogo />
  <GitLogo />
  <GithubLogo class="[&_path]:fill-foreground" />
  <GoLogo />
  <BashLogo />
  <RustLogo class="fill-foreground" />
  <AstroLogo class="fill-foreground" />
  <NodeJsLogo />
  <SvelteLogo />
</div>
