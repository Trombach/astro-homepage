---
import AngularLogo from "icons:astro/logos/angular-icon";
import AstroLogo from "icons:astro/logos/astro-icon";
import BashLogo from "icons:astro/logos/bash-icon";
import Css3Logo from "icons:astro/logos/css-3";
import GitLogo from "icons:astro/logos/git-icon";
import GithubLogo from "icons:astro/logos/github-icon";
import GoLogo from "icons:astro/logos/go";
import Html5Logo from "icons:astro/logos/html-5";
import JavaScriptLogo from "icons:astro/logos/javascript";
import NextJsLogo from "icons:astro/logos/nextjs-icon";
import NodeJsLogo from "icons:astro/logos/nodejs-icon";
import RustLogo from "icons:astro/logos/rust";
import SvelteLogo from "icons:astro/logos/svelte-icon";
import TailwindLogo from "icons:astro/logos/tailwindcss-icon";
import TypeScriptLogo from "icons:astro/logos/typescript-icon";
---

<div class="grid grid-cols-3 gap-14 lg:grid-cols-5 [&>svg]:size-full">
  <TypeScriptLogo />
  <AngularLogo />
  <JavaScriptLogo />
  <Html5Logo class="fill-foreground" />
  <Css3Logo class="fill-foreground" />
  <TailwindLogo />
  <NextJsLogo />
  <GitLogo />
  <GithubLogo class="[&_path]:fill-foreground" />
  <GoLogo />
  <BashLogo />
  <RustLogo class="fill-foreground" />
  <AstroLogo class="fill-foreground" />
  <NodeJsLogo />
  <SvelteLogo />
</div>
